{
	"name": "海投",
	"version": "1.0",
	"manifest_version": 2,
	"description": "简历投递助手",
	"permissions": ["http://*/*", "https://*/*","notifications" ],
	"browser_action": {
		"default_icon": "icon.png"
	},
	"content_scripts": [
		{
			"matches": ["http://*/*","https://*/*" ],
			"css":["css/haitou.css"],
			"js": ["js/jquery.js",  "js/jquery.md5.js","js/upload.js","js/dialog.js", "js/loader.js"],
			"run_at":"document_end"
		}
	],
	"web_accessible_resources": [ "css/*", "js/*" ]
}
